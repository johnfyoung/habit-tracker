{
  "name": "habit-tracker",
  "version": "1.0.0",
  "description": "Habit Tracker with Express backend and React frontend",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "start": "cd backend && npm start",
    "server:dev": "cd backend && npm run dev",
    "client:dev": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "install": "cd backend && npm install && cd ../frontend && npm install"
  },
  "keywords": [
    "habit-tracker",
    "express",
    "react",
    "mongodb"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^6.2.0"
  },
  "dependencies": {
    "react-responsive": "^10.0.0"
  }
}
